title <- textGrob("Title",gp=gpar(fontsize=16))
# padding <- unit(25,"mm")
#
# table <- gtable_add_rows(
#   t1,
#   #heights = grobHeight(title) + padding,
#   heights = grobHeight(t2) + padding,
#   pos = 0)
